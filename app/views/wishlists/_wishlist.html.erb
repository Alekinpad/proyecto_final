<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Icon</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wishlists.each do |wishlist| %>
      <tr>
        <td><%= wishlist.name %></td>
        <td><%= wishlist.icon %></td>
        <td><%= @user %></td>
        <td><%= link_to 'Show', [@user, wishlist] %></td>
        <td><%= link_to 'Edit', edit_user_wishlist_path(@user, wishlist) %></td>
        <td><%= link_to 'Destroy', [@user, wishlist], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>